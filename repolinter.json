{
  "extends": "https://raw.githubusercontent.com/todogroup/repolinter/master/rulesets/default.json",
  "rules": {
    "source-license-headers-exist": {
      "level": "error",
      "rule": {
        "type": "file-starts-with",
        "options": {
          "globsAll": [
            "**/*.py",
            "**/*.js",
            "**/*.c",
            "**/*.cc",
            "**/*.cpp",
            "**/*.h",
            "**/*.ts",
            "**/*.sh",
            "**/*.rs"
          ],
          "skip-paths-matching": {
            "patterns": [
              "TrainingExtensions/tensorflow/src/python/aimet_tensorflow/graph_editor/*",
              "TrainingExtensions/tensorflow/src/python/aimet_tensorflow/quantize/*"

            ]
          },
          "lineCount": 40,
          "patterns": [
            "Copyright.*Qualcomm Innovation Center, Inc|(c).*Qualcomm Innovation Center, Inc",
            "SPDX-License-Identifier"
          ],
          "flags": "i"
        }
      }
    }
  }
}
